/* query is to tell dbs to give exactly what piece of information you wanna collect from dbs
*/
CREATE TABLE STUDENT(
STUDENT_ID INT PRIMARY KEY,
NAME VARCHAR(20),
PROFFF VARCHAR(20)
);
/* or can be also be written as */
CREATE TABLE STUDENT(
STUDENT_ID INT,
NAME VARCHAR(20),
PROFFF VARCHAR(20),
PRIMARY KEY(STUDENT_ID);
);
